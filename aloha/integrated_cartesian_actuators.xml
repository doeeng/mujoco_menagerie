<mujoco>
<!-- The dynamical parameters in this file are not the result of system identification.
They were chosen for MJPC efficiency. The resulting qfrc is still limited by every joint's actuator_frcrange,
as specified in the main model. -->
  <default>
    <default class="act">
      <general dyntype="filter" biastype="affine" dynprm="1"/>
      <default class="act_position">
        <general gainprm="1000" biasprm="0 -1000 -100" ctrlrange="-0.5 0.5"/>
      </default>
      <default class="act_rotation">
        <general gainprm="10" biasprm="0 -10 -2" ctrlrange="-1.3 1.3"/>
      </default>
      <default class="act_gripper">
        <general gainprm="365" biasprm="0 -365 -50" dynprm="0.1" ctrlrange="0.002 0.037"/>
      </default>
    </default>
  </default>

  <worldbody>
    <site name="actuation_center" pos="0 0 0.25"/>
  </worldbody>

  <actuator>
    <general class="act_position" name="left/X" site="left/gripper" refsite="actuation_center" gear="1 0 0 0 0 0"/>
    <general class="act_position" name="left/Y" site="left/gripper" refsite="actuation_center" gear="0 1 0 0 0 0"/>
    <general class="act_position" name="left/Z" site="left/gripper" refsite="actuation_center" gear="0 0 1 0 0 0"/>
    <general class="act_rotation" name="left/RX" site="left/gripper" refsite="actuation_center" gear="0 0 0 1 0 0"/>
    <general class="act_rotation" name="left/RY" site="left/gripper" refsite="actuation_center" gear="0 0 0 0 1 0"/>
    <general class="act_rotation" name="left/RZ" site="left/gripper" refsite="actuation_center" gear="0 0 0 0 0 1"/>
    <general class="act_gripper" name="left/finger" joint="left/left_finger"/>
    <general class="act_position" name="right/X" site="right/gripper" refsite="actuation_center" gear="1 0 0 0 0 0"/>
    <general class="act_position" name="right/Y" site="right/gripper" refsite="actuation_center" gear="0 1 0 0 0 0"/>
    <general class="act_position" name="right/Z" site="right/gripper" refsite="actuation_center" gear="0 0 1 0 0 0"/>
    <general class="act_rotation" name="right/RX" site="right/gripper" refsite="actuation_center" gear="0 0 0 1 0 0"/>
    <general class="act_rotation" name="right/RY" site="right/gripper" refsite="actuation_center" gear="0 0 0 0 1 0"/>
    <general class="act_rotation" name="right/RZ" site="right/gripper_flipped" refsite="actuation_center" gear="0 0 0 0 0 1"/>
    <general class="act_gripper" name="right/finger" joint="right/left_finger"/>
  </actuator>
</mujoco>
